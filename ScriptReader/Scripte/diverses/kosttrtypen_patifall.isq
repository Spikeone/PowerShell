set dateformat dmy
go
set nocount on
go
print ' ----------------------------------------------------- '
print ' Kostenträgerstatistik der Patientenfälle im Jahr 2000 '
print ' ----------------------------------------------------- '
select KOSTTRTYP, count(*) as "ANZAHL"
from PATIFALL where ERFASSDAT between '01.01.2000' and '31.12.2000'
group by KOSTTRTYP
go  
