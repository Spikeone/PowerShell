set dateformat dmy
go
select 
 T.LAUF,
 A.AUFTRAGNR,
 T.TSTATUS,
 PR.PROBEEXTID,
 PR.PSTATUS,
 E.EINSCODE,
 A.ERFASSDAT,
 A.LABEINDAT,
 A.AUFTRAGDAT,
 PE.PERSONID,
 F.FORMULARCODE,
 Auftragstorno=A.STORNODAT,
 Probenstorno=PR.STORNODAT
from 
 AUFTRAG A,
 PERSON PE,
 PROBE PR,
 TASKFLAG T,
 EINSENDER E,
 FORMULAR F
where
 A.AUFTRAGX=T.XREF 
 and PE.PERSONX = A.ERFASSPERS 
 and E.EINSENDERX = A.EINSENDERX
 and PR.AUFTRAGX = A.AUFTRAGX
 and F.FORMULARX =* A.FORMULARX
order by A.ERFASSDAT 
go 

