set dateformat dmy
go
select F.FORMULARCODE, A.AUFTRAGNR, A.EINSENDERX, A.SQLERFASSDAT, A.AUFTRAGDAT, A.LABEINDAT, A.PLANDAT, A.ABNAHMEDAT, E.EINSCODE, PS.NAME
from AUFTRAG A, TASKFLAG T, EINSENDER E, PATISTAMM PS, FORMULAR F
where
A.AUFTRAGX=T.XREF
and T.TSTATUS=0
and E.EINSENDERX=A.EINSENDERX
and PS.PATISTAMMX=A.PATISTAMMX
and F.FORMULARX=A.FORMULARX
order by A.ERFASSDAT
go
