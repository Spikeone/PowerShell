select
  AU.AUFTRAGNR,
  TS.TSTATUS,
  AB.ABLINR,
  AB.ABLICODE,
  APO.POSITION,
  PR.PROBEEXTID,
  PR.PSTATUS
from TASKFLAG TS, AUFTRAG AU, PROBE PR, ABLI AB, ABLIPOS APO
where
  TS.TSTATUS = 1 
  and AU.AUFTRAGX = TS.XREF 
  and PR.AUFTRAGX = AU.AUFTRAGX 
  and PR.PKENNUNG = 3 
  and PR.PSTATUS = 0
  and APO.AUFTRAGX = AU.AUFTRAGX
  and APO.PROBEX = PR.PROBEX
  and AB.ABLIX = APO.ABLIX
go  